{
    "name": "webappick/wp-list-info",
    "description": "Everything you need about WordPress info",
    "type": "library",
    "require-dev": {
        "woocommerce/woocommerce": "5.8",
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.6",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "codeatcode/codeatcs": "^1.0",
        "wp-coding-standards/wpcs": "dev-develop",
        "szepeviktor/phpstan-wordpress": "^1.3",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "WebAppick\\WPListInfo\\": "src/"
        }
    },
    "scripts": {
        "cs-check": "./vendor/bin/phpcs --standard=phpcs.xml",
        "cs-fix": "./vendor/bin/phpcbf",
        "phpstan": "./vendor/bin/phpstan analyse"
    },
    "authors": [
        {
            "name": "webappick",
            "email": "webappick@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "automattic/jetpack-autoloader": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
